{
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "storybook/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "storybook-static" }
    }
  ],
  "routes": [
    {
      "src": "/[0-9]+$",
      "dest": "/"
    },
    {
      "src": "/storybook",
      "status": 301,
      "headers": { "Location": "/storybook/" }
    }
  ],
  "github": {
    "autoAlias": true,
    "enabled": true
  },
  "public": true
}
